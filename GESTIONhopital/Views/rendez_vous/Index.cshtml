@model IEnumerable<GESTIONhopital.Models.rendez_vous>

 
<h2>Liste des rendez vous</h2>

<p>
    @Html.ActionLink("Ajouter un rendez vous", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Medcin.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Passion.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.date)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Medcin.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Passion.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.date)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                @Html.ActionLink("Details", "Details", new { id = item.id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.id })
            </td>
        </tr>
    }

</table>
